{
  "build": {
    "beforeDevCommand": "",
    "beforeBuildCommand": "",
    "devUrl": "http://localhost:1420",
    "distDir": "../app",
    "withGlobalTauri": true
  },
  "package": {
    "productName": "ApplyDiff-Refactored",
    "version": "0.2.0"
  },
  "tauri": {
    "bundle": {
      "active": true,
      "targets": "all",
      "identifier": "com.applydiff.refactored",
      "icon": [
        "icons/32x32.png",
        "icons/128x128.png",
        "icons/128x128@2x.png",
        "icons/icon.icns",
        "icons/icon.ico"
      ]
    },
    "security": {
      "csp": null
    },
    "windows": [
      {
        "fullscreen": false,
        "resizable": true,
        "title": "ApplyDiff",
        "width": 1120,
        "height": 800,
        "minWidth": 800,
        "minHeight": 600
      }
    ],
    "systemTray": null,
    "allowlist": {
      "all": false,
      "dialog": {
        "all": false,
        "open": true,
        "save": false,
        "message": false,
        "ask": false,
        "confirm": false
      },
      "clipboard": {
        "all": true
      }
    }
  }
}