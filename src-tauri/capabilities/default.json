{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "enables the default permissions", 
  "windows": ["main"],
  "permissions": [
    "core:default",
    "core:window:default",
    "dialog:default",
    "clipboard-manager:default",
    "fs:default",
    "fs:allow-read-file",
    "fs:allow-write-file",
    "fs:allow-exists"
  ]
}
