{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "pach",
  "version": "0.1.0",
  "identifier": "com.pach.dev",
  "build": {
    "frontendDist": "../src"
  },
  "app": {
    "windows": [
      {
        "title": "Pach",
        "width": 400,
        "height": 600,
        "resizable": false,
        "fullscreen": false,
        "alwaysOnTop": true,
        "decorations": false
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": "all"
  }
}